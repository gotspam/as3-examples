{
	"class": "AS3",
	"action": "deploy",
	"declaration": {
		"class": "ADC",
		"schemaVersion": "3.0.0",
		"id": "service_4_id",
		"label": "service_4_label",
		"remark": "service_4_remark",
		"Service4": {
			"class": "Tenant",
			"Service_4": {
				"class": "Application",
				"template": "https",
				"serviceMain": {
					"class": "Service_HTTPS",
					"virtualAddresses": [
						"10.0.1.89"
					],
					"snat": "auto",
					"pool": "Pool1",
					"profileHTTP": {
						"use": "HTTP_Profile"
					},
					"profileCompressionHTTP": {
						"use": "HTTP_Compress"
					},
					"serverTLS": "TLS_Offload",
					"policyWAF": {
						"bigip": "/Common/waf_baseCredentials"
					}
				},
				"Pool1": {
					"class": "Pool",
					"monitors": [
						"http"
					],
					"members": [{
							"servicePort": 80,
							"serverAddresses": [
								"18.205.1.169"
							]
						},
						{
							"servicePort": 80,
							"serverAddresses": [
								"34.239.240.82"
							]
						}
					]
				},
				"HTTP_Profile": {
					"class": "HTTP_Profile",
					"adcName": "Service4",
					"xForwardedFor": true
				},
				"HTTP_Compress": {
					"class": "HTTP_Compress",
					"cpuSaver": true,
					"cpuSaverHigh": 70,
					"cpuSaverLow": 50,
					"selective": true
				},
				"TLS_Offload": {
					"class": "TLS_Server",
					"certificates": [{
						"certificate": "/Common/Student12-clientss.prf"
					}]
				}
			}
		}
	}
}
